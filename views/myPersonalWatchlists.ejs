<div class="container">

  <div class="list-container">
    <% myPersonalWatchlists.forEach(s => { %>

    <div class="subscriber">
      <p>
        <span class="item-label">Name:</span>
        <%= s.name %>
      </p>
      <p>
        <span class="item-label">Genre:</span>
        <%= s.genre %>
      </p>
      <p>
        <span class="item-label">Intended Audience:</span>
        <%= s.intendedAudience %>
      </p>
    </div>

    <% }); %>
  </div>
</div>
